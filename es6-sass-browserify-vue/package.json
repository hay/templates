{
  "name": "test-template",
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:autoprefixer": "postcss app/css/style.css --use autoprefixer --no-map -r",
    "build:css": "npm run build:sass && npm run build:autoprefixer",
    "build:js" : "browserify app/js/app.js -t babelify -o app/bundle.js",
    "build:sass": "node-sass app/css/scss/style.scss app/css/style.css",
    "watch": "chokidar 'app/css/scss/*.scss' -c 'npm run build:css' & chokidar 'app/js/*.js' -c 'npm run build:js'"
  },
  "dependencies": {
    "valenski": "^1.3.0",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "chokidar-cli": "^1.2.0",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.22",
    "postcss-cli": "^5.0.0",
    "watch": "^1.0.2"
  }
}