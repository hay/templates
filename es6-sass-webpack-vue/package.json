{
  "name": "test-template",
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:autoprefixer": "postcss app/css/style.css --use autoprefixer --no-map -r",
    "build:css": "npm run build:sass && npm run build:autoprefixer",
    "build:js": "webpack -d",
    "build:sass": "node-sass app/css/scss/style.scss app/css/style.css",
    "clean" : "rm -rf dist",
    "clean:after" : "rm -rf dist/css/scss dist/js",
    "dist" : "npm run clean && webpack -p && cp -rv app dist && npm run clean:after",
    "postinstall" : "npm run build",
    "serve": "cd app && http-server",
    "watch": "chokidar 'app/css/scss/*.scss' -c 'npm run build:css' & webpack -dw"
  },
  "dependencies": {
    "es6-promise": "^4.2.4",
    "intersection-observer": "^0.5.0",
    "mdn-polyfills": "^5.6.0",
    "regenerator-runtime": "^0.11.1",
    "valenski": "^1.3.0",
    "vue": "^2.5.16",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chokidar-cli": "^1.2.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.22",
    "postcss-cli": "^5.0.0",
    "vue-loader": "^15.0.9",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.1",
    "webpack-cli": "^3.1.1"
  }
}